<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhangxiaoyu01.mydemo">

    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.SwipeRefreshLayoutActivity" />
        <activity
            android:name=".Activity.IndicatorActivity"
            android:label="@string/title_activity_incaditor" />
        <activity
            android:name=".Activity.SlidingMenuActivity"
            android:label="@string/title_activity_sliding_menu" />
        <activity
            android:name=".Activity.ProgressActivity"
            android:label="@string/title_activity_progress" />
        <activity
            android:name=".Activity.WuziqiActivity"
            android:label="@string/title_activity_wuziqi" />
        <activity
            android:name=".Activity.LoopAnimationActivity"
            android:label="@string/title_activity_loop_animation" />
        <activity
            android:name=".Activity.DampViewActivity"
            android:label="@string/title_activity_damp_view" />
        <activity
            android:name=".Activity.CircleViewActivity"
            android:label="@string/title_activity_circle_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.ToolbarActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity android:name=".Activity.SearchResultActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.CoordinatorLayoutActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".Activity.CalanderActivity"
            android:label="@string/title_activity_calander"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activity.ToggleActivity" />
        <activity android:name=".Activity.MvpDaggerActivity" />
        <activity android:name=".Activity.RetrofitActivity" />
        <activity android:name=".Activity.ChoosePeriodActivity" />
        <activity android:name=".Activity.YunActivity"></activity>
    </application>

</manifest>